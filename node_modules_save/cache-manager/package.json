{
  "name": "cache-manager",
  "version": "5.0.1",
  "description": "Cache module for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/node-cache-manager/node-cache-manager.git"
  },
  "keywords": [
    "cache",
    "redis",
    "lru-cache",
    "memory cache",
    "multiple cache"
  ],
  "authors": [
    {
      "name": "Bryan Donovan"
    },
    {
      "name": "Juan Aguilar Santillana",
      "email": "mhpoin@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lru-cache": "^7.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@faker-js/faker": "7.5.0",
    "@release-it/conventional-changelog": "5.1.1",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitest/coverage-c8": "0.24.0",
    "dotenv-cli": "6.0.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "release-it": "15.5.0",
    "typescript": "4.8.4",
    "vitest": "0.24.0"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{md,json,yml}": "prettier --write"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "vitest",
    "release": "pnpm check && pnpm test -- --run && pnpm build && dotenv release-it",
    "fix": "pnpm lint && pnpm fmt",
    "check": "pnpm lint:check && pnpm fmt:check",
    "lint": "eslint --cache --max-warnings 0 --fix '**/*.ts'",
    "fmt": "prettier --write '**/*.{md,json,yml}'",
    "lint:check": "eslint --cache --max-warnings 0 '**/*.ts'",
    "fmt:check": "prettier --check '**/*.{md,json,yml}'"
  }
}